const defaultValue = {
	width: {
		length: '',
		unit: '',
	},
	min_width: {
		length: '',
		unit: '',
	},
	max_width: {
		length: '',
		unit: '',
	},
	height: {
		length: '',
		unit: '',
	},
	min_height: {
		length: '',
		unit: '',
	},
	max_height: {
		length: '',
		unit: '',
	},
}
const value = { ...defaultValue, ...data.value }

const fieldConfig = name => ( {
	name: data.name + '[][' + name + '][length]',
	value: value[name].length,
	unit_name: data.name + '[][' + name + '][unit]',
	unit_value: value[name].unit,
	field: {
		units: [ 'px', '%', 'em', 'rem', 'vw', 'vh' ],
		slider: {
			min: 0,
			max: 1500,
			step: 1,
		},
	},
} );
